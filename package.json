{
  "name": "@ticketswap/solar",
  "private": false,
  "version": "0.0.0-development",
  "license": "MIT",
  "repository": "https://github.com/TicketSwap/solar",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "commit": "git-cz",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "yarn clean",
    "type-check": "tsc",
    "dev": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:esm && npm run build:cjs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:esm": "babel src --out-dir dist --extensions '.ts,.tsx' --ignore 'src/**/*.stories.tsx','src/**/*.test.tsx'",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir dist/cjs --extensions '.ts,.tsx' --ignore 'src/**/*.stories.tsx','src/**/*.test.tsx'",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "NODE_ENV=test jest test --coverage",
    "test:watch": "NODE_ENV=test jest test --watch",
    "lint": "eslint src",
    "prepublishOnly": "yarn build",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|scss|ts|tsx|md|mdx|graphql)\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "build-storybook": "build-storybook",
    "format": "npm run prettier -- --write",
    "validate": "npm run test && npm run prettier -- --list-different",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@ticketswap/comets": ">=2.10.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "prettier": {
    "bracketSpacing": true,
    "bracketSameLine": false,
    "semi": false,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@reach/accordion": "^0.16.1",
    "@reach/auto-id": "^0.16.0",
    "@reach/dialog": "^0.16.2",
    "@reach/menu-button": "^0.16.2",
    "@reach/tabs": "^0.12.1",
    "@reach/tooltip": "^0.16.2",
    "@reach/utils": "^0.16.0",
    "compute-scroll-into-view": "^1.0.16",
    "downshift": "^6.1.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/jest": "^11.5.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@ticketswap/comets": "^2.14.1",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^2.5.1",
    "is-ci-cli": "^2.1.2",
    "jest": "^27.3.1",
    "jest-runner-eslint": "^1.0.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "prettier": "^2.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "semantic-release": "^18.0.0",
    "size-limit": "^6.0.1",
    "storybook-addon-theme-toggle": "^0.0.1",
    "storybook-addon-themes": "^6.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
