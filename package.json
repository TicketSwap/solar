{
  "name": "@ticketswap/solar",
  "private": false,
  "version": "0.0.0-development",
  "scripts": {
    "commit": "git-cz",
    "start": "rollup -c --watch",
    "build": "rollup -c --environment NODE_ENV:production",
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "dev": "start-storybook -p 9001 -c .storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|scss|ts|tsx|md|mdx|graphql)\"",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "format": "npm run prettier -- --write",
    "validate": "npm run test && npm run prettier -- --list-different",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "downshift": "^3.2.10",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-spring": "^7.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "@storybook/react": "^5.1.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.13",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "downshift": "^3.2.10",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.1",
    "is-ci-cli": "^1.1.1",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "jest-emotion": "^10.0.11",
    "jest-runner-eslint": "^0.7.4",
    "jest-watch-select-projects": "^0.1.1",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-spring": "7.2.10",
    "@testing-library/react": "^8.0.1",
    "rollup": "^1.15.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-filesize": "^6.1.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-visualizer": "^1.1.1",
    "rollup-watch": "^4.3.1",
    "semantic-release": "^15.13.12"
  },
  "main": "dist/solar.js",
  "module": "dist/solar.es.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": "https://github.com/TicketSwap/solar",
