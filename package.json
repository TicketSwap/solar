{
  "name": "@ticketswap/solar",
  "private": false,
  "version": "0.0.0-development",
  "license": "MIT",
  "repository": "https://github.com/TicketSwap/solar",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "commit": "git-cz",
    "dev": "tsdx watch",
    "build": "tsdx build --environment NODE_ENV:production",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest test --coverage",
    "test:watch": "jest test --watch",
    "lint": "tsdx lint",
    "prepublishOnly": "tsdx build",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|scss|ts|tsx|md|mdx|graphql)\"",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "build-storybook": "build-storybook",
    "format": "npm run prettier -- --write",
    "validate": "npm run test && npm run prettier -- --list-different",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "post-merge": "yarn install --frozen-lockfile",
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS =~ l$ ]]; then yarn install --frozen-lockfile; fi",
      "post-rebase": "yarn install"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": false,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "module": "dist/solar.es.js",
  "dependencies": {
    "@reach/accordion": "^0.12.1",
    "@reach/auto-id": "^0.12.1",
    "@reach/dialog": "^0.12.1",
    "@reach/menu-button": "^0.12.1",
    "@reach/tabs": "^0.12.1",
    "@reach/tooltip": "^0.12.1",
    "@reach/utils": "^0.12.1",
    "@ticketswap/comets": "^2.11.1",
    "compute-scroll-into-view": "^1.0.16",
    "downshift": "^6.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.10.4",
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/jest": "^11.1.0",
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "@size-limit/preset-small-lib": "^4.7.0",
    "@storybook/addon-essentials": "6.1.15",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/addons": "^6.0.28",
    "@storybook/react": "^6.1.16",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^16.9.9",
    "babel-loader": "^8.2.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "3.1.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.6.3",
    "jest-runner-eslint": "^0.10.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "semantic-release": "^17.2.3",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
