{
  "name": "@ticketswap/solar",
  "private": false,
  "version": "0.0.0-development",
  "scripts": {
    "commit": "git-cz",
    "start": "rollup -c --watch",
    "build": "rollup -c --environment NODE_ENV:production",
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "dev": "start-storybook -p 9001 -c .storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|scss|ts|tsx|md|mdx|graphql)\"",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "format": "npm run prettier -- --write",
    "validate": "npm run test && npm run prettier -- --list-different",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.34",
    "@emotion/styled": "^10.0.27",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.26",
    "@storybook/storybook-deployer": "^2.8.6",
    "@storybook/theming": "^6.0.26",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "3.1.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.5.3",
    "jest-emotion": "^10.0.32",
    "jest-runner-eslint": "^0.10.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.29.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-visualizer": "^4.1.1",
    "rollup-watch": "^4.3.1",
    "semantic-release": "^17.2.1"
  },
  "main": "dist/solar.js",
  "module": "dist/solar.es.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": "https://github.com/TicketSwap/solar",
  "dependencies": {
    "@reach/accordion": "^0.11.2",
    "@reach/auto-id": "^0.11.2",
    "@reach/dialog": "^0.11.2",
    "@reach/tabs": "^0.11.2",
    "@reach/tooltip": "^0.11.2",
    "@reach/utils": "^0.11.2",
    "@ticketswap/comets": "^2.9.0",
    "compute-scroll-into-view": "^1.0.16",
    "downshift": "^6.0.6"
  }
}
