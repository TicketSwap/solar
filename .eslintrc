{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["jsx-a11y", "react-hooks"],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "extends": [
    "react-app",
    "eslint-config-prettier",
    "plugin:jsx-a11y/recommended"
  ],
  "env": {
    "browser": true
  }
}
